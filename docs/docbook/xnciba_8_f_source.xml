<anchor xml:id="_xnciba_8_f_source_1l00042"/>00042 &#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="keywordflow">IF</emphasis>(radius.LT.radin1)&#32;<emphasis role="keywordflow">GOTO</emphasis>&#32;999
<anchor xml:id="_xnciba_8_f_source_1l00043"/>00043 <emphasis role="comment">*</emphasis>
<anchor xml:id="_xnciba_8_f_source_1l00044"/>00044 <emphasis role="comment">*&#32;---&#32;Find&#32;the&#32;right&#32;bar&#32;to&#32;check&#32;with</emphasis>
<anchor xml:id="_xnciba_8_f_source_1l00045"/>00045 &#32;&#32;&#32;&#32;&#32;&#32;ang&#32;=&#32;atan2(yrel,xrel)
<anchor xml:id="_xnciba_8_f_source_1l00046"/>00046 &#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="keywordflow">IF</emphasis>(ang.LT.0.)&#32;ang=ang+2*pi
<anchor xml:id="_xnciba_8_f_source_1l00047"/>00047 &#32;&#32;&#32;&#32;&#32;&#32;iang&#32;=&#32;nint((ang-angcr1*0.5)/angcr1)+1
<anchor xml:id="_xnciba_8_f_source_1l00048"/>00048 &#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="keywordflow">IF</emphasis>(iang.GT.ncrow1)&#32;iang&#32;=&#32;1
<anchor xml:id="_xnciba_8_f_source_1l00049"/>00049 &#32;&#32;&#32;&#32;&#32;&#32;xfuel&#32;=&#32;xycro1(1,iang)
<anchor xml:id="_xnciba_8_f_source_1l00050"/>00050 &#32;&#32;&#32;&#32;&#32;&#32;yfuel&#32;=&#32;xycro1(2,iang)
<anchor xml:id="_xnciba_8_f_source_1l00051"/>00051 &#32;&#32;&#32;&#32;&#32;&#32;radrel&#32;=&#32;sqrt((xrel-xfuel)**2+(yrel-yfuel)**2)
<anchor xml:id="_xnciba_8_f_source_1l00052"/>00052 &#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="keywordflow">IF</emphasis>(radrel.LE.radfue)&#32;<emphasis role="keywordflow">THEN</emphasis>
<anchor xml:id="_xnciba_8_f_source_1l00053"/>00053 &#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;imatb&#32;=&#32;3
<anchor xml:id="_xnciba_8_f_source_1l00054"/>00054 &#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;xpofue&#32;=&#32;xfuel
<anchor xml:id="_xnciba_8_f_source_1l00055"/>00055 &#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;ypofue&#32;=&#32;yfuel
<anchor xml:id="_xnciba_8_f_source_1l00056"/>00056 &#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;ipofue&#32;=&#32;iang
<anchor xml:id="_xnciba_8_f_source_1l00057"/>00057 <emphasis role="keywordflow">&#32;&#32;&#32;&#32;&#32;&#32;ENDIF</emphasis>
<anchor xml:id="_xnciba_8_f_source_1l00058"/>00058 &#32;&#32;&#32;&#32;&#32;&#32;<emphasis role="keywordflow">GOTO</emphasis>&#32;999
<anchor xml:id="_xnciba_8_f_source_1l00059"/>00059 <emphasis role="comment">*</emphasis>
<anchor xml:id="_xnciba_8_f_source_1l00060"/>00060 &#32;999&#32;&#32;<emphasis role="keyword">END</emphasis>
